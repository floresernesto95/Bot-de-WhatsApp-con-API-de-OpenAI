# Meta API Configuration
# Get your access token from Meta App Dashboard > API Setup
# Or create a System User access token from Meta Business Settings: https://business.facebook.com/settings/system-users
ACCESS_TOKEN="your_access_token_here"
APP_ID="your_app_id_here"
APP_SECRET="your_app_secret_here"
PHONE_NUMBER_ID="your_phone_number_id_here"
VERIFY_TOKEN="your_verify_token_here"
VERSION="v21.0"  # API version - update as needed
RECIPIENT_WAID="your_whatsapp_number_with_country_code"  # Format: +xxxxxxxxxxxx

# Django Configuration
DJANGO_SECRET_KEY="your_django_secret_key_here"

# Database Configuration
DB_NAME="your_database_name"
DB_USER="your_database_user"
DB_PASSWORD="your_database_password"
DB_HOST="127.0.0.1"
DB_PORT="5432"

# OpenAI Configuration
OPENAI_API_KEY="your_openai_api_key_here"

# System Configuration
SYSTEM_PROMPT="your_system_prompt_here"